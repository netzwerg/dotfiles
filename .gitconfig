[core]
	excludesfile = /Users/netzwerg/.gitignore_global
  autocrlf = input
	commentchar = |
[user]
	email = rahel.luethy@fhnw.ch
	name = Rahel LuÌˆthy
[color]
  ui = auto
[alias]
  lg = log --all --graph --pretty=format:'%Cblue%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr %an)%Creset' --abbrev-commit --date=relative
  st = status
  ci = commit
  br = branch
  co = checkout
  dt = difftool
  dts = difftool --staged
[push]
  default = simple
[merge]
  tool = sourcetree
[diff]
  tool = sourcetree
[difftool]
  prompt = false
[mergetool]
  prompt = false
  keepBackup = false
[difftool "sourcetree"]
	cmd = /usr/local/bin/ksdiff -w \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /usr/local/bin/ksdiff --merge --output \"$MERGED\" --base \"$BASE\" -- \"$LOCAL\" --snapshot \"$REMOTE\" --snapshot
	trustExitCode = true
[credential]
	helper = osxkeychain
[http]
	sslVerify = false
[commit]
	template = /Users/netzwerg/.stCommitMsg
[pull]
	rebase = false
[init]
	defaultBranch = main
